{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [true, "single"],
      "no-console": false,
      "trailing-comma": [true, {"multiline": "always", "singleline": "never"}],
      "variable-name": [true, "allow-snake-case", "allow-pascal-case"],
      "curly": [true, "ignore-same-line"]
    },
    "rulesDirectory": []
}
